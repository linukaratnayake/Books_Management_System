<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="423.0" prefWidth="393.0" xmlns="http://javafx.com/javafx/15.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.LoginNewUserController">
   <Label layoutX="94.0" layoutY="42.0" text="Login | Create New User">
      <font>
         <Font name="System Bold" size="18.0"/>
      </font>
   </Label>
   <Label layoutX="36.0" layoutY="103.0" text="Username:">
      <font>
         <Font size="14.0"/>
      </font>
   </Label>
   <Label layoutX="38.0" layoutY="174.0" text="Password:">
      <font>
         <Font size="14.0"/>
      </font>
   </Label>
   <Label layoutX="36.0" layoutY="251.0" text="Confirm Password:">
      <font>
         <Font size="14.0"/>
      </font>
   </Label>
   <TextField fx:id="txtNewUsername" layoutX="168.0" layoutY="100.0" onKeyTyped="#checkUsername" prefHeight="26.0"
              prefWidth="188.0" promptText="Enter Username."/>
   <PasswordField fx:id="txtNewPassword" layoutX="168.0" layoutY="171.0" prefHeight="26.0" prefWidth="188.0"
                  promptText="Enter Password."/>
   <PasswordField fx:id="txtNewConfirmPassword" layoutX="168.0" layoutY="248.0" prefHeight="26.0" prefWidth="188.0"
                  promptText="Confirm Entered Password."/>
   <Button layoutX="267.0" layoutY="334.0" mnemonicParsing="false" onAction="#btnSignUpClicked" prefHeight="38.0"
           prefWidth="100.0" text="Sign Up">
      <font>
         <Font name="System Bold" size="16.0"/>
      </font>
   </Button>
   <Button layoutX="146.0" layoutY="335.0" mnemonicParsing="false" onAction="#btnResetClicked" prefWidth="100.0"
           text="Reset">
      <font>
         <Font name="System Bold" size="16.0"/>
      </font>
   </Button>
   <Button fx:id="btnCancel" layoutX="26.0" layoutY="335.0" mnemonicParsing="false" onAction="#btnCancelClicked"
           prefWidth="100.0" text="Cancel">
      <font>
         <Font name="System Bold" size="16.0"/>
      </font>
   </Button>
</AnchorPane>
